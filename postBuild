git clone https://github.com/thelfer/tfel.git --branch TFEL-4.0.0 --single-branch
cd tfel
ls
cmake . -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=../install -Denable-python=ON -Denable-python-bindings=ON -DPYTHON_INCLUDE_DIR=$(python3 -c "from distutils.sysconfig import get_python_inc; print(get_python_inc())")  -DPYTHON_LIBRARY=$(python3 -c "import distutils.sysconfig as sysconfig; print(sysconfig.get_config_var('LIBDIR'))") -DPYTHON_EXECUTABLE:FILEPATH=`which python3`
make
make install
cd ..
git clone https://github.com/thelfer/tfel.git
cd tfel
jupyter serverextension enable --py nbgitpuller --sys-prefix
export PYVISTA_TRAME_SERVER_PROXY_PREFIX="$JUPYTERHUB_SERVICE_PREFIX/proxy/"
